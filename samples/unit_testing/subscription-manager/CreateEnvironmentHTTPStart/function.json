{
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "authLevel": "anonymous",
        "name": "req",
        "type": "httpTrigger",
        "direction": "in",
        "route": "product/{clientName}",
        "methods": [
          "post",
          "get"
        ]
      },
      {
        "name": "$return",
